<?xml version="1.0" encoding="utf-8"?>
<LinearLayout tools:context=".ServicioWeb3Activity"
    android:orientation="vertical"
    android:layout_height="match_parent"
    android:layout_width="match_parent"
    android:id="@+id/LinearLayout1"
    android:paddingTop="26dp"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <TextView android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:id="@+id/textView"
        android:gravity="center"
        android:text="@string/indicacion_actualizar"/>
    <EditText android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:id="@+id/editText_fechaL"
        android:gravity="center"
        android:inputType="date"
        android:hint="@string/formato_fecha"
        android:ems="10">
        <requestFocus/>
    </EditText>
    <LinearLayout
        android:orientation="vertical"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:paddingTop="26dp"
        android:gravity="center">
        <Button
            android:id="@+id/btn_Local_Local"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/botonSW"
            android:onClick="LservicioPHP"
            android:text="@string/servicio_PHPlocal" />

        <Button
            android:id="@+id/btn_Local_Externo"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:backgroundTint="@color/botonSW"
            android:onClick="LservicioPHP"
            android:text="@string/servicio_PHPexterno" />

        <Button
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:id="@+id/button3"
            android:text="@string/guardar"
            android:backgroundTint="@color/botonI"
            android:onClick="guardar"/>
    </LinearLayout>

    <ListView
        android:paddingTop="15dp"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        android:id="@+id/listViewlocales"> </ListView>
</LinearLayout>